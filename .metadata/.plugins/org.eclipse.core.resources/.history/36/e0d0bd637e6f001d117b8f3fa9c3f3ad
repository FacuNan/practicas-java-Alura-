package javaPrimerosPasos;

public class TipoVariable {
public static void main (String[] args) {
	
double variable1 = 230.89;

int variable1Entero = (int) variable1;




long prueba = 122222222222222L; //* Long acepta numeros de haste 2 elevado a la 31;

short numeroPequeno = 13555; //numeros de hasta 2 elevado hasta 16;
 
byte masPequeno = 114;//soporta hasta bytes

float numeroMuchoMasPequeno = 2.5F; //numeros decimales peque√±os

double resultado = (int) variable1 + variable1Entero;
System.out.println(resultado)
	
}
}